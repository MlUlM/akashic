{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"wasm": {
			"type": "text",
			"path": "script/akashic.wasm",
			"global": true
		},
		"font": {
			"type": "image",
			"path": "image/font.png",
			"width": 512,
			"height": 220
		},
		"font_glyphs": {
			"type": "text",
			"path": "text/font_glyphs.json"
		},
		"assets/shaders/post_processing.wgsl": {
			"type": "text",
			"path": "assets/shaders/post_processing.wgsl",
			"global": true
		},
		"assets/shaders/shader.wgsl": {
			"type": "text",
			"path": "assets/shaders/shader.wgsl",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"multi_admission"
			]
		}
	}
}