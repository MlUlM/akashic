{
  "width": 1280,
  "height": 720,
  "fps": 30,
  "main": "./script/main.js",
  "assets": {
    "player": {
      "type": "image",
      "width": 80,
      "height": 80,
      "path": "image/player.png"
    },
    "shot": {
      "type": "image",
      "width": 16,
      "height": 16,
      "path": "image/shot.png"
    },
    "se": {
      "type": "audio",
      "path": "audio/se",
      "systemId": "sound",
      "duration": 250
    },
    "main": {
      "type": "script",
      "path": "script/main.js",
      "global": true
    },
    "crypto": {
      "type": "script",
      "path": "script/crypto.js",
      "global": true
    }
  },
  "moduleMainScripts": {
    "crypto": "node_modules/crypto/index.js"
  },
  "environment": {
    "sandbox-runtime": "3",
    "niconico": {
      "supportedModes": [
        "multi_admission"
      ]
    }
  }
}