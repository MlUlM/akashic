{
  "name": "typescript-game-sample",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm run build",
    "scan": "akashic scan asset",
    "build": "rimraf ./script && tsc && akashic-cli-scan asset script",
    "update": "akashic-cli-scan asset && akashic-cli-scan globalScripts",
    "lint": "eslint -c .eslintrc.js \"src/**/*.ts\" --fix",
    "typescript-start": "rimraf ./script && tsc && akashic-cli-scan asset script && akashic-sandbox . -p 3001",
    "nicolive": "akashic serve . --target-service nicolive --port 3301",
    "export-zip": "rimraf game.zip && akashic-cli-export zip -o game.zip -s",
    "export-html": "rimraf game && akashic-cli-export html -o game",
    "test": "jest"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@akashic/akashic-cli-export": "^1.7.51",
    "@akashic/akashic-cli-scan": "^0.16.0",
    "@akashic/akashic-engine": "^3.6.0",
    "@akashic/akashic-sandbox": "^0.24.0",
    "@akashic/eslint-config": "^1.1.0",
    "@akashic/headless-akashic": "^4.0.0",
    "@types/jest": "^29.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.1.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@akashic-extension/akashic-box2d": "^3.1.0"
  }
}
